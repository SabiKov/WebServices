package co.sabi11.toy.model;

public class Toy {
	
    private String id;
    private String name;
    private String category;
    private String brand;
    private String price;
    private String quantity;
	
    //Default Constructor
    public toy() {}
    
    public Toy(String id, String name, String category, String brand, String price, String quantity) {
		super();
		this.id = id;
		this.name = name;
		this.category = category;
		this.brand = brand;
		this.price = price;
		this.quantity = quantity;
	}

}
